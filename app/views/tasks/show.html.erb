  <h1><%= @task.title %></h1>
  <p><%= @task.details %></p>
  <% if @task.completed %>
    <input type="checkbox" id="/" name="/" checked>
  <% else %>
      <input type="checkbox" id="/" name="/">
  <% end %>
  <%= link_to "Back to list", tasks_path %>
  <%= link_to "Edit this task", edit_path(@task)  %>


